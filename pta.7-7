#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) 
{
	int m,n,i,j;
	char f;
	scanf("%d%c%d",&m,&f,&n);
	if(m>12)
	{
		printf("%d:%d PM",m-12,n);
		return 0;
	}
	else if(m==12)
	{
		printf("%d:%d PM",m,n);
		return 0;
	}
	else if(m<12){
		printf("%d:%d AM",m,n);
		return 0;
	}
	return 0;
}
