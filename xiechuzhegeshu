#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc, char *argv[]) {
	char str[101];
	int i,s=0;
	gets(str);
	/*printf("%d\n",strlen(str));*/
	for(i=0;i<strlen(str);i++) 
	{
		if(str[i]=='1')
		s=s+1;
		if(str[i]=='2')
		s=s+2;
		if(str[i]=='3')
		s=s+3;
		if(str[i]=='4')
		s=s+4;
		if(str[i]=='5')
		s=s+5;
		if(str[i]=='6')
		s=s+6;
		if(str[i]=='7')
		s=s+7;
		if(str[i]=='8')
		s=s+8;
		if(str[i]=='9')
		s=s+9;
		if(str[i]=='0')
		s=s+0;
	}
	
	if(s/100!=0)
	{
		int k=s/100;
		if(k==1)
		printf("yi");
		if(k==2)
		printf("er");
		if(k==3)
		printf("san");
		if(k==4)
		printf("si");
		if(k==5)
		printf("wu");
		if(k==6)
		printf("liu");
		if(k==7)
		printf("qi");
		if(k==8)
		printf("ba");
		if(k==9)
		printf("jiu");
		s=s-k*100;
		printf(" ");
	}
	if(s/10!=0)
	{
		int h=s/10;
		if(h==1)
		printf("yi");
		if(h==2)
		printf("er");
		if(h==3)
		printf("san");
		if(h==4)
		printf("si");
		if(h==5)
		printf("wu");
		if(h==6)
		printf("liu");
		if(h==7)
		printf("qi");
		if(h==8)
		printf("ba");
		if(h==9)
		printf("jiu");
		s=s-h*10;
		printf(" ");
	}
	if(s/1!=0)
	{
		int p=s/1;
		if(p==1)
		printf("yi");
		if(p==2)
		printf("er");
		if(p==3)
		printf("san");
		if(p==4)
		printf("si");
		if(p==5)
		printf("wu");
		if(p==6)
		printf("liu");
		if(p==7)
		printf("qi");
		if(p==8)
		printf("ba");
		if(p==9)
		printf("jiu");
	}
	return 0;
}
